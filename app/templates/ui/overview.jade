div.overview
  div.overview-image
    img.image__background(src="images/overview.png")
  div.property-card
    div.property-card__header
      h2.header__name Pahang Homes for Flood Victims
      h3.header__address Jerantut, Pahang, Malaysia
    div.property-card__description
      div.description__money
        h2.money__raised MYR 100,000
        h3.money__goal raised out of MYR 100,000
      div.description__data
        ul.data__list
          li.list__item
            span.item__left
              img(src="", alt="").left__img
            span.item__right Affordable Housing
          li.list__item
            span.item__left 4.25%
            span.item__right Project Returns  
          li.list__item
            span.item__left 3.5 months
            span.item__right Project Timeline
    div.property-card__status 
      div.status__bar
          h2.status__progress 100% FUNDED - Project in progress
  div.overview-footer
    .overview-footer__item
      img.item__img(src="", alt="")
      span.item__text Details
    .overview-footer__item
      img.item__img(src="", alt="")
      span.item__text Pictures & Videos

    
